{
  "name": "thewired-bitbucket",
  "version": "2.3.0",
  "author": "thewired",
  "summary": "Install Atlassian Bitbucket Server",
  "license": "MIT",
  "source": "https://github.com/danofthewired/puppet-bitbucket.git",
  "project_page": "https://github.com/danofthewired/puppet-bitbucket",
  "issues_url": "https://github.com/danofthewired/puppet-bitbucket/issues",
  "description": "Module to install Atlassian Bitbucket Server/Data Center. On-premises source code management for Git that's secure, fast, and enterprise grade.",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04",
        "16.04",
        "17.04",
        "18.04"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7",
        "8",
        "9"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.0.0"
    },
    {
      "name": "puppetlabs/inifile",
      "version_requirement": ">= 1.0.0"
    },
    {
      "name": "mkrakowitzer/deploy",
      "version_requirement": ">= 0.0.3"
    },
    {
      "name": "puppet/staging",
      "version_requirement": ">= 1.0.0"
    },
    {
      "name": "puppet/archive",
      "version_requirement": ">= 0.4.4"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=5.5.10 < 8.0.0"
    }
  ]
}
